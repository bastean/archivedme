{
	"private": true,
	"name": "satori-cli",
	"version": "1.1.0",
	"description": "Crypto Market directly from the command-line and through an interactive interface. Powered by CoinGecko API",
	"author": "Bastean",
	"license": "MIT",
	"keywords": [
		"satori",
		"command",
		"cli",
		"crypto"
	],
	"homepage": "https://github.com/bastean/satori-cli#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/bastean/satori-cli.git"
	},
	"bugs": {
		"url": "https://github.com/bastean/satori-cli/issues"
	},
	"files": [
		"src/*"
	],
	"type": "module",
	"main": "index.js",
	"bin": "index.js",
	"scripts": {
		"start": "node --experimental-specifier-resolution=node --no-warnings index.js",
		"dev": "nodemon --no-stdin --experimental-specifier-resolution=node --no-warnings index.js"
	},
	"dependencies": {
		"@coingecko/cryptoformat": "0.5.4",
		"axios": "1.1.2",
		"colors": "1.4.0",
		"commander": "9.4.1",
		"figlet": "1.5.2",
		"inquirer": "9.1.3",
		"table": "6.8.0"
	},
	"devDependencies": {
		"nodemon": "2.0.20"
	},
	"imports": {
		"#src/*": "./src/*",
		"#cli": "./src/cli",
		"#cli/*": "./src/cli/*",
		"#libs": "./src/libs",
		"#libs/*": "./src/libs/*",
		"#services": "./src/services",
		"#services/*": "./src/services/*",
		"#utils": "./src/utils",
		"#utils/*": "./src/utils/*",
		"#main": "./src/main"
	}
}
