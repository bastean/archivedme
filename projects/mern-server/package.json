{
	"private": true,
	"name": "mern-server",
	"version": "1.4.1",
	"description": "Basis REST API for a quick start in development with Node.js, Express, MongoDB, and Much more.",
	"author": "Bastean",
	"license": "MIT",
	"homepage": "https://github.com/bastean/mern-server#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/bastean/mern-server.git"
	},
	"bugs": {
		"url": "https://github.com/bastean/mern-server/issues"
	},
	"engines": {
		"node": ">=18",
		"npm": ">=8"
	},
	"type": "module",
	"scripts": {
		"start": "node --experimental-specifier-resolution=node --no-warnings index.js",
		"dev": "nodemon --experimental-specifier-resolution=node --no-warnings index.js"
	},
	"dependencies": {
		"axios": "1.1.3",
		"bcrypt": "5.1.0",
		"colors": "1.4.0",
		"compression": "1.7.4",
		"cors": "2.8.5",
		"dotenv": "16.0.3",
		"express": "4.18.2",
		"express-rate-limit": "6.6.0",
		"express-slow-down": "1.5.0",
		"figlet": "1.5.2",
		"helmet": "6.0.0",
		"jsonwebtoken": "8.5.1",
		"mongoose": "6.7.1",
		"nodemailer": "6.8.0"
	},
	"devDependencies": {
		"nodemon": "2.0.20"
	},
	"imports": {
		"#src/*": "./src/*",
		"#database": "./src/database",
		"#database/*": "./src/database/*",
		"#helpers": "./src/helpers",
		"#helpers/*": "./src/helpers/*",
		"#server": "./src/server",
		"#server/*": "./src/server/*",
		"#main": "./src/main",
		"#api/*": "./src/api/v1/*",
		"#assets": "./src/api/v1/assets",
		"#assets/*": "./src/api/v1/assets/*",
		"#controllers": "./src/api/v1/controllers",
		"#controllers/*": "./src/api/v1/controllers/*",
		"#libs": "./src/api/v1/libs",
		"#libs/*": "./src/api/v1/libs/*",
		"#middlewares": "./src/api/v1/middlewares",
		"#middlewares/*": "./src/api/v1/middlewares/*",
		"#models": "./src/api/v1/models",
		"#models/*": "./src/api/v1/models/*",
		"#routes": "./src/api/v1/routes",
		"#routes/*": "./src/api/v1/routes/*",
		"#services": "./src/api/v1/services",
		"#services/*": "./src/api/v1/services/*",
		"#utils": "./src/api/v1/utils",
		"#utils/*": "./src/api/v1/utils/*",
		"#validations": "./src/api/v1/validations",
		"#validations/*": "./src/api/v1/validations/*"
	}
}
